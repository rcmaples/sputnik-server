    "Chai"
  ],
  "author": "RC Maples",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rcmaples/sputnik-server/issues"
  },
  "homepage": "https://github.com/rcmaples/sputnik-server#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.4.0",
    "lodash.filter": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.isboolean": "^3.0.3",
    "lodash.mapkeys": "^4.6.0",
