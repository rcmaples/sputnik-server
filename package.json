{
  "name": "sputnik-server",
  "version": "0.0.1",
  "description": "Backend Node/Express server for SputnikApp",
  "main": "server.js",
  "scripts": {
    "start": "export NODE_ENV=production && node server.js",
    "dev": "export NODE_ENV=development && nodemon server.js",
    "test": "export NODE_ENV=test && mocha test/*.test.js --exit",
    "test-watch": "nodemon --exec \"npm run test\""
  },
